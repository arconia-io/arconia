# Environment Post Processors
org.springframework.boot.env.EnvironmentPostProcessor=\
io.arconia.opentelemetry.autoconfigure.config.ActuatorEnvironmentPostProcessor,\
io.arconia.opentelemetry.autoconfigure.config.SpringBootEnvironmentPostProcessor,\
io.arconia.opentelemetry.autoconfigure.config.OpenTelemetrySdkEnvironmentPostProcessor

# Connection Details Factories
org.springframework.boot.autoconfigure.service.connection.ConnectionDetailsFactory=\
io.arconia.opentelemetry.testcontainers.LgtmOtlpLoggingContainerConnectionDetailsFactory,\
io.arconia.opentelemetry.testcontainers.LgtmOtlpMetricsContainerConnectionDetailsFactory,\
io.arconia.opentelemetry.testcontainers.LgtmOtlpTracingContainerConnectionDetailsFactory
