# Environment Post Processors
org.springframework.boot.EnvironmentPostProcessor=\
io.arconia.opentelemetry.autoconfigure.config.OpenTelemetryEnvironmentPostProcessor,\
io.arconia.opentelemetry.autoconfigure.config.SpringBootEnvironmentPostProcessor

# Connection Details Factories
org.springframework.boot.autoconfigure.service.connection.ConnectionDetailsFactory=\
io.arconia.opentelemetry.testcontainers.LgtmOtlpLoggingContainerConnectionDetailsFactory,\
io.arconia.opentelemetry.testcontainers.LgtmOtlpMetricsContainerConnectionDetailsFactory,\
io.arconia.opentelemetry.testcontainers.LgtmOtlpTracingContainerConnectionDetailsFactory,\
io.arconia.opentelemetry.testcontainers.OtelCollectorOtlpLoggingContainerConnectionDetailsFactory,\
io.arconia.opentelemetry.testcontainers.OtelCollectorOtlpMetricsContainerConnectionDetailsFactory,\
io.arconia.opentelemetry.testcontainers.OtelCollectorOtlpTracingContainerConnectionDetailsFactory,\
io.arconia.opentelemetry.testcontainers.PhoenixOtlpTracingContainerConnectionDetailsFactory

# Application Listeners
org.springframework.context.ApplicationListener=\
io.arconia.opentelemetry.autoconfigure.traces.bridge.MicrometerTracingOpenTelemetryContextStorageApplicationListener
